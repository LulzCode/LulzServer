		contents = null;
		file = null;
	}
	
	public LulzFile(File theFile, ArrayList<String> readFile){
		contents = readFile;
		file = theFile;
	}
	
	public ArrayList<String> read(){
		
		return null;
	}
	
	public void update(ArrayList<String> readFile){
		file = readFile;
	}
	
	public void getLocation(){
		
	}
	
}
